@model WebApplicationGrupp13.Models.InviteUserModel

@{
    ViewBag.Title = "InviteUsers";
}

<h2>Bjud in mötesdeltagare</h2>
<p>Välj deltagare att bjuda in till mötet</p>
<hr />

<div>
    @using (Html.BeginForm("AddInvitedUsersToList", "InviteUsers", FormMethod.Post, new { meetId = Model.MeetingId }))
    {
        @Html.HiddenFor(m => m.MeetingId)
        @Html.ListBoxFor(m => m.SelectedUsers, Model.AllUsers, new { size = 5, style = "max-width: 400px; width: 400px" })
        <br />
        <br />
        <input type="submit" value="Bjud in" class="btn btn-default" />
    }
</div>

@* enctype = "multipart/form-data",*@ 